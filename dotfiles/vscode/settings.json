{
    "interactive-smartlog.pull": "arc pull",
    "interactive-smartlog.fetch": "jf get",
    "interactive-smartlog.right-drawer-width": 992,
    "interactive-smartlog.bottom-drawer-height": 500,
    "interactive-smartlog.autoRunMergeDrivers": true,
    "[cpp]": {
        "editor.defaultFormatter": "nuclide.cpp"
    },
    "fb.ms-remote-connections.hosts": [],
    "fb.ms-remote-connections.odtypes": [
        {
            "type": "ondemand_fbcode",
            "flavor": "default",
            "folders": [
                "/data/sandcastle/boxes/fbsource/"
            ]
        },
        {
            "type": "ondemand_configerator",
            "flavor": "default",
            "folders": []
        },
        {
            "type": "ondemand_www",
            "flavor": "default",
            "folders": []
        },
        {
            "type": "ondemand_dataswarm",
            "flavor": "default",
            "folders": []
        },
        {
            "type": "ondemand_bento",
            "flavor": "default",
            "folders": []
        },
        {
            "type": "ondemand_bento",
            "flavor": "bento_passwordless",
            "folders": []
        },
        {
            "type": "ondemand_fbcode",
            "flavor": "de",
            "folders": []
        },
        {
            "type": "ondemand_gpu",
            "flavor": "default",
            "folders": []
        }
    ],
    "omnisearch.resultLimit": 20,
    "hg.jumpToNextConflictOnSave": "Jump On Save And Open ISL When Done",

    "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.darwin": "~/.config/nvim/init.lua",
    "vscode-neovim.logOutputToConsole": true,
    "whichkey.bindings": [
        {
            "key": "d",
            "name": "show uncommitted changes",
            "type": "command",
            "command": "fb-hg.diff"
        },
        {
            "key": "l",
            "name": "show/hide tnterative smartlog",
            "type": "command",
            "command": "fb-hg.toggle-interactive-smartlog"
        },
        {
            "key": "f",
            "name": "focus on file explorer",
            "type": "command",
            "command": "workbench.files.action.focusFilesExplorer",
            "when": "!fileexplorerfocus"
        },
        {
            "key": "t",
            "name": "toggle terminal",
            "type": "command",
            "command": "workbench.action.togglePanel"
        },
        {
            "key": "e",
            "name": "toggle explore sidebar",
            "type": "command",
            "command": "workbench.action.toggleSideBarVisibility"
        },
        {
            "key": "g",
            "name": "open a biggrep pane",
            "type": "command",
            "command": "nuclide-biggrep:open-pane"
        },
        {
            "key": "c",
            "name": "open in codehub",
            "type": "command",
            "command": "fb-phabricator-diffusion:openInCodeHub",
        },
        {
            "key": "o",
            "name": "open omni-search",
            "type": "command",
            "command": "omni-search-ui:quick-search",
        }
    ],
    "terminal.integrated.profiles.linux": {
        "tmux-shell": {
          "path": "tmux",
          "args": ["new-session", "-A", "-s", "vscode:${workspaceFolder}"]
        }
    },
    "terminal.integrated.profiles.osx": {
        "tmux-shell": {
          "path": "tmux",
          "args": ["new-session", "-A", "-s", "vscode:${workspaceFolder}"]
        }
    },
    "terminal.integrated.defaultProfile.linux": "tmux",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.defaultProfile.osx": "tmux",
    "terminal.integrated.systemd.shouldUse": true,

    "window.zoomLevel": 2,
    "search.showLineNumbers": true,

    "editor.fontSize": 13,
    "editor.semanticHighlighting.enabled": true,
    "editor.formatOnPaste": true,
    "editor.minimap.showSlider": "always",
    "editor.suggest.insertMode": "replace",
    "editor.suggest.showStatusBar": true,
    "editor.accessibilitySupport": "off",
    "editor.lineNumbers": "relative",
    "editor.suggestSelection": "first",
    "editor.suggest.snippetsPreventQuickSuggestions": false,

    "workbench.startupEditor": "none",
    "workbench.colorTheme": "Solarized Dark",
    "workbench.editor.showTabs": false,
    "wasabi.interactiveDevelopment.introMessageNumTimesShown": 10,
    "wasabi.interactiveDevelopment.disableIntroMessage": true,
    "arcinfer.runOnSave": true,
    "meta.global.history.enabled": true,
    "editor.inlineSuggest.allowSuggestOnTriggerCharacters": true,
    "window.restoreWindows": "none",
    "terminal.integrated.fontFamily": "monospace",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.lineHeight": 1.1,
    "terminal.integrated.scrollback": 100000,
}


